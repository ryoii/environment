# normal
alias ..='cd ..'
alias c='clear'
alias du='du -h'
alias df='df -h'

alias ls='ls --color=auto'
alias dir='dir --color=auto'
alias ll='ls -l'
alias la='ls -A'
alias lla='ls -lA'


# net
alias ports='netstat -tulanp'

# sudo
alias apt='sudo apt'

# wget
alias wget='wget -c'

# python
alias py='python3'
alias py2='python2'
